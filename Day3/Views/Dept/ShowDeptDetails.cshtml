@{
}

@model List<Department>
<table class="table table-bordered table-hover" width="50%">
    <tr>
        <th>
            Department Name
        </th>
        <th>
            Manager Name
        </th>
    </tr>
    @foreach(var item in Model)
    {
        <tr> 
            <td>@item.Name</td>
            <td>@item.Manager</td>
        </tr>
    }
</table>

<a href="\Dept\Add" class="btn btn-alert-success">Add new Department</a>